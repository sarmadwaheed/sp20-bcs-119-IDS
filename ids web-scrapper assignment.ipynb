    "import html\n",
    "from bs4 import BeautifulSoup\n",
    "import requests\n",
    "response = requests.get(\"https://www.samaa.tv/\")\n",
    "soup = BeautifulSoup(response.text, 'lxml')\n",
    "search=soup.findAll(\"h2\")\n",
    "for tags in search:\n",
    "    headlines=tags.text.strip()\n",
    "    print(headlines)"
   ]
